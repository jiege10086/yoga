<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniu.mapper.TDynamicMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.woniu.domain.TDynamic">
        <result column="dyn_id" property="dynId" />
        <result column="open_time" property="openTime" />
        <result column="open_detail" property="openDetail" />
        <result column="imgs" property="imgs" />
        <result column="peo_name" property="peoName" />
        <result column="uuid" property="uuid" />
        <result column="peo_role" property="peoRole" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        dyn_id, open_time, open_detail, imgs, peo_name, uuid, peo_role
    </sql>

</mapper>
